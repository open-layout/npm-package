{
  "name": "open-layout",
  "version": "1.0.2",
  "description": "OpenLayout npm package to easily create your projects based on a public template.",
  "main": "build/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc && cross-env NODE_ENV=production node build/index.js",
    "startd": "tsc && cross-env NODE_ENV=development node build/index.js",
    "just-start": "cross-env NODE_ENV=production node build/index.js",
    "jsd": "cross-env NODE_ENV=development node build/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/index.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "version": "npm version"
  },
  "bin": {
    "open-layout": "build/index.js",
    "olayout": "build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-layout/npm-package.git"
  },
  "keywords": [
    "open-layout",
    "templates",
    "npm-package"
  ],
  "engines": {
    "node": ">=16"
  },
  "author": "IMXNOOBX",
  "license": "CC-BY-NC-4.0",
  "bugs": {
    "url": "https://github.com/open-layout/npm-package/issues"
  },
  "homepage": "https://github.com/open-layout/npm-package#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/figlet": "^1.5.8",
    "@types/shelljs": "^0.8.15",
    "axios": "^1.6.7",
    "color-support": "^1.1.3",
    "enquirer": "^2.4.1",
    "figlet": "^1.7.0",
    "kleur": "^4.1.5",
    "nanospinner": "^1.1.0",
    "shelljs": "^0.8.5"
  }
}
