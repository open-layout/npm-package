{
  "name": "open-layout",
  "version": "1.0.0",
  "description": "OpenLayout npm package to easily create your projects based on a public template.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production tsc && node ./dist/index.js",
    "just-start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node ./src/index.ts",
    "build": "cross-env NODE_ENV=production tsc",
    "version": "npm version"
  },
  "bin": {
    "open-layout": "dist/index.js",
    "olayout": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-layout/npm-package.git"
  },
  "keywords": [
    "open-layout",
    "templates",
    "npm-package"
  ],
  "author": "imxnoobx, azaldevx, anglesad",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/open-layout/npm-package/issues"
  },
  "homepage": "https://github.com/open-layout/npm-package#readme",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "enquirer": "^2.4.1",
    "kleur": "^4.1.5"
  }
}
